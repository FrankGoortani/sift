{
    "code_extensions": [
        ".py", ".js", ".ts", ".java", ".cpp", ".c", ".cc", ".cxx", ".hpp", ".h",
        ".cs", ".rb", ".go", ".rs", ".php", ".swift", ".kt", ".kts", ".scala",
        ".pl", ".pm", ".r", ".sh", ".bash", ".bat", ".ps1", ".m", ".mm", ".vb",
        ".vbs", ".html", ".css", ".scss", ".less", ".xml", ".xsl", ".xslt",
        ".jsonnet", ".lua"
    ],
    "config_extensions": [
        ".json", ".yaml", ".yml", ".ini", ".env", ".cfg", ".conf", ".toml",
        ".plist", ".properties", ".settings", ".hcl", ".xml", ".editorconfig",
        ".csproj", ".sln", ".gradle", ".maven", ".gemspec", ".dockerfile",
        "docker-compose.yml", ".makefile", ".cmake", ".bazel", ".bzl"
    ],
    "documentation_extensions": [
        ".md", ".rst", ".txt", ".adoc", ".asciidoc", ".docx", ".odt", ".pdf",
        ".tex", ".rtf", ".epub"
    ],
    "default_model":"gpt-4o-mini",
    "max_tokens": 80000,
    "token_split_overlap": 500,
    "sift_extension": "_sift_summary.md",
    "default_exclusions": ["node_modules", "vendor", "__pycache__"]
}
